MODULE MURL;
IMPORT SYSTEM, MUI IN "MicroUI.ob07", RL IN "./Raylib.ob07";

CONST
	NULL = 0;


PROCEDURE [extern, "murl_setup_font_ex"] SetupFontEx*(ctx: MUI.Context; font: RL.Font);
